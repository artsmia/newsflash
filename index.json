---
thanks: "https://github.com/christian-fei/Simple-Jekyll-Search/blob/master/search.json"
---
[{% for post in site.posts %}
  {
    "title"    : "{{ post.title | escape }}",
    "object": "{{post.object}}",
    "image": "{{post.image}}",
    "link"     : "http://newsflash.dx.artsmia.org{{ post.url }}",
    "date"     : "{{ post.date }}"
  } {% if forloop.last %}{% else %},{% endif %}
{% endfor %}]
